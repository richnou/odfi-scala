#!/bin/bash

## This script lists the dependencies of the provided artifact, plus itself
## Usage:
##  odfi_mvn_list_dependencies "groupId:artifactId:version"
## Output:
##   path/to/dep1.jar
#    


#mvn org.apache.maven.plugins:maven-dependency-plugin:2.6:get -Dartifact="org.odfi.collaboration.docbook:docbook-core:1.2-SNAPSHOT"


mvn org.apache.maven.plugins:maven-dependency-plugin:2.6:list -DoutputAbsoluteArtifactFilename=true